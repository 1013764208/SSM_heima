<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">


    <bean id="bookDao" class="com.item.dao.impl.BookDaoImpl" scope="prototype"/>
    <bean id="bookService" name="bookService2 bookService3" class="com.item.service.impl.BookServiceImpl">
        <property name="bookDao" ref="bookDao"/>
    </bean>

    <!--方式四：使用FactoryBean实例bean-->
    <bean id = "userDao" class="com.item.factory.UserDaoFactoryBean"/>
</beans>